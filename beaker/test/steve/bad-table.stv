foreign def puts(char[]) -> int;

Port p1 = ":5000";

layout eth
{
	src : uint(48);
	dst : uint(48);
	type : uint(16);
}


decoder start eth_d(eth)
{
  extract eth.dst;
  extract eth.type;
  goto switch;
}

var bad_var : uint(48) = 0;

exact_table switch(eth.dst, eth.type)
{
  { eth.dst, 800 } ->
  {
    output p1;
  }

  miss ->
  {
    output p1;
  }
}
