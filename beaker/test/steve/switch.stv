foreign def puts(char[]) -> int;

Port p1 = ":5000";

layout eth
{
	src : uint(48);
	dst : uint(48);
	type : uint(16);
}


decoder start eth_d(eth)
{
  extract eth::dst;
  extract eth::type;
  goto switch;
}

exact_table switch(eth::dst, eth::type)
{
  { 0x00_00_00_00_00_00, 800 } ->
  {
    output p1;
  }

  miss ->
  {
    output p1;
  }
}
