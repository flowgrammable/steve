foreign def puts(char[]) -> int;

Port p1 = ":5000";

layout eth
{
	dst : uint(48);
	src : uint(48);
	type : uint(16);
}


decoder start eth_d(eth)
{
  extract eth.dst;
  extract eth.type;
  goto switch;
}

exact_table switch(eth.dst, eth.type)
{
	[	timeout = 1000 ]
  { 0x12_34_56_78_90_ab, 0x0800 } ->
  {
		puts("hit1");
    output p1;
  }

	{ 0xab_12_34_56_78_90, 0x0800 } ->
  {
		puts("hit2");
    output p1;
  }

  miss ->
  {
    output p1;
  }
}
