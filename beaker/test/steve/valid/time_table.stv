layout eth
{
	dst : uint(48);
	src : uint(48);
	type : uint(16);
}


decoder start eth_d(eth)
{
  extract eth.dst;

  // Time insert by inserting an entry 1000 times.
  var i : int = 0;
  while (i < 10000000)
  {
    i = i + 1;
    insert { i } -> { } into t1;
  }

  // Time remove by removing the entries 1000 times.
  var r : int = 0;
  while (r < 10000000)
  {
    r = r + 1;
    remove { r } from t1;
  }
}


exact_table t1(eth.dst)
{

}
