foreign def puts(char[]) -> int;

Port p1 = 1;

layout eth
{
	dst : uint(48);
	src : uint(48);
	type : uint(16);
}


decoder start eth_d(eth)
{
  extract eth.dst;
  extract eth.type;
  remove miss from t1;
  goto t1;
}

exact_table t1(eth.type)
{
  miss ->
  {
		puts("Miss...in t1.");
    drop;
  }
}
