foreign def puts(char[]) -> int;

Port p1 = ":5000";

layout eth
{
	src : uint(48);
	dst : uint(48);
	type : uint(16);
}


decoder start eth_d(eth)
{
  extract eth::dst;
  extract eth::type;
  goto switch;
}

exact_table switch(eth::dst, eth::type)
{
  miss ->
  {
		puts("Miss");

    add
    { eth::dst, eth::type } ->
    {
      output p1;
    }
    into switch;

    output p1;
  }
}
