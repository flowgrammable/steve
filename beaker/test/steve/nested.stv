foreign def puts(char[]) -> int;

layout eth
{
  src : uint(48);
  dst : uint(48);
  type : uint(16);
  n : nested;
  m : uint;
}


layout nested
{
  nest : nested2;
  v : int(16);
}


layout nested2
{
  n : uint;
}

decoder start eth_d(eth)
{
  extract eth.n.v;
  extract eth.n.nest.n;

  eth.n.v;
  eth.n.nest.n;

  drop;
}

exact_table t1(eth.n.v, eth.n.nest.n)
{
  {0, 0} ->
  {
    drop;
  }
}
